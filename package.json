{
  "name": "vue-ts-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --mode dev",
    "build": "vite build && node toFile.mjs",
    "build:dev-pure": "vue-tsc && vite build --mode development",
    "build:test": "vue-tsc && vite build --mode test",
    "build:dev": "vue-tsc --noEmit && vite build --mode development && node toFile.mjs",
    "build:sit": "vue-tsc --noEmit && vite build --mode sit && node toFile.mjs",
    "build:pre": "vue-tsc --noEmit && vite build --mode pre && node toFile.mjs",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod && node toFile.mjs",
    "preview": "vite preview",
    "eslint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "format": "prettier --write \"./**/*.{ts,tsx,js,json,vue,html,md}\"",
    "lint:eslint": "eslint src/**/*.{ts,vue} --cache --fix",
    "lint:style": "stylelint src/**/*.{less,scss,md,vue,,json,ts,tsx} --cache --fix",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "preinstall": "node ./scripts/preinstall.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@iconify/iconify": "^3.1.1",
    "@vueuse/core": "^6.2.0",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.1.3",
    "qs": "^6.12.0",
    "resize-observer-polyfill": "^1.5.1",
    "simplex-noise": "^4.0.1",
    "sortablejs": "^1.15.2",
    "unocss": "^0.58.5",
    "vditor": "^3.8.6",
    "vue": "^3.2.47",
    "vue-i18n": "9.1.7",
    "vue-json-pretty": "^2.0.4",
    "vue-router": "^4.2.1",
    "vue-scrollto": "^2.20.0",
    "vue-types": "^4.0.3",
    "xgplayer": "^3.0.18",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.21.8",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@purge-icons/generated": "^0.7.0",
    "@types/crypto-js": "^4.0.2",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "dotenv": "^16.4.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.0",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "terser": "^5.19.3",
    "typescript": "^5.4.4",
    "vite": "5.1.3",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.0",
    "vite-plugin-imagemin": "^0.4.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-vue-setup-extend": "^0.1.0",
    "vue-tsc": "^1.8.27"
  }
}
